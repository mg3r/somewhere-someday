{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin.html",
      "headers": {
        "WWW-Authenticate": "Basic realm=\"Secure Area\"",
        "Content-Security-Policy": "frame-ancestors 'none'"
      },
      "continue": true
    },
    {
      "src": "/admin.html",
      "status": 401,
      "has": [
        {
          "type": "header",
          "key": "Authorization",
          "value": "(?!Basic YWRtaW46aWFtc3BlY2lhbA==)"
        }
      ]
    },
    {
      "src": "/archive",
      "dest": "/archive.html"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1"
    },
    {
      "src": "/scripts/(.*)",
      "dest": "/scripts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}